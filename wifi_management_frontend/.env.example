# Example environment variables for the WiFi Management Frontend (CRA)
# Do not commit real secrets.

# Base URL for future backend REST API
REACT_APP_API_BASE=https://api.example.com

# Alternative backend URL supported by this app (used if API_BASE is unset)
REACT_APP_BACKEND_URL=https://api.example.com

# WebSocket URL for future realtime integration
REACT_APP_WS_URL=wss://api.example.com/ws

# Optional override; typically CRA uses NODE_ENV
REACT_APP_NODE_ENV=development

# Feature flags in JSON or CSV:
# JSON: {"alerts":true,"advancedVlan":false}
# CSV: alerts,advancedVlan
REACT_APP_FEATURE_FLAGS=alerts,advancedVlan
